def fib ::= 位 x : int . x <= 1 ? x : fib (x-1) + fib (x-2);

printline(fib(7));

printline(fib(3));


def spaceForward ::= 位 pos : int . pos+8;
def equalPos ::= 位 x y : bool . ((y-x == 8)) ? True : False ;
def pawnMoveForward ::= 位 x boolean : bool . boolean == True ? spaceForward(x) : x;

printline(pawnMoveForward(1, 2, False));
