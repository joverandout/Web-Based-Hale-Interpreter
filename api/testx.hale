def fib ::= 位 x : int . x <= 1 ? x : fib (x-1) + fib (x-2);

printline(fib(7));

printline(fib(3));


def spaceForward ::= 位 list : do . [listhead(list), listend(list)+1];
def equalPos ::= 位 x1 y1 x2 y2 : bool . (x1 == x2) & (y1 == y2) ? True : False ;
def pawnMoveForward ::= 位 x y boolean : bool . boolean == True ? spaceForward([x,y]) : [];

printline(pawnMoveForward(1, 2, False));
